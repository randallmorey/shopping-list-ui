    "clean:coverage": "rm -rf coverage_*",
    "pretest": "npm run clean:coverage && npm run lint",
    "test": "COVERAGE=true ember exam --split=4 --parallel=1 --random",
    "posttest": "ember coverage-merge && npm run clean:coverage"
