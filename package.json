  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "build:production": "ember build --environment production",
    "deploy": "firebase deploy --project $FIREBASE_PROJECT_ID",
    "precommit": "npm test",
    "commit": "git-cz",
    "lint": "npm run lint:licenses && npm run lint:hbs && npm run lint:js && npm run lint:sass",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "lint:licenses": "license-checker --onlyAllow 'Apache*;Apache-2.0;Apache License, Version 2.0;BSD;BSD-2-Clause;BSD-3-Clause;CC0-1.0;CC-BY-3.0;CC-BY-4.0;ISC;MIT;MPL-2.0;Public Domain;Unlicense' --excludePackages 'buffers@0.1.1;cycle@1.0.3;npm@5.1.0;shopping-list-ui@0.0.0;xmldom@0.1.27'",
    "lint:sass": "sass-lint -v -q",
    "start": "ember serve",
    "start:firebase": "USE_FIREBASE=true npm start",
    "clean:coverage": "rm -rf coverage_*",
    "pretest": "npm run clean:coverage && npm run lint",
    "test": "COVERAGE=true ember exam --random",
    "posttest": "ember coverage-merge && npm run clean:coverage"
  },
  "devDependencies": {
    "@ember/edition-utils": "^1.1.1",
    "@ember/optional-features": "^1.1.0",
